<div class="mod <% if $Image %>photo<% end_if %>">
<% if $ShowHeading %> 
    <div>
    	<% if $Link %>
			<a href="$Link">
				
		        <h3>$Title</h3>

		    	<% if $FeaturedInitiative && $AssociatedPage.ID == 60 %>

                	<img src="data:image/png;base64," data-src="$FeaturedInitiative.MainImage.CroppedImage(350,197).URL" alt="$Title">
		        <% else_if $Image %>
		        	<img src="data:image/png;base64," data-src="$Image.SetWidth(279).URL" alt="An image representing $Title">
		        <% end_if %>
	    	</a>
    	<% else %>
		
	        <h3>$Title</h3>
	        
	        <% if $Image %>
	        	<img src="data:image/png;base64," data-src="$Image.URL" alt="An image representing $Title">
	        <% end_if %>
    	<% end_if %>
    </div>
<% end_if %>
    
    <% if RSSFeedLink %>
    	<% include SidebarRssFeed %>
    <% end_if %>
    $Content
</div>
